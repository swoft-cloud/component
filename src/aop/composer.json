{
    "name": "swoft/aop",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "aop"
    ],
    "description": "Aop Component for Swoft",
    "license": "Apache-2.0",
    "require": {
        "swoft/framework": "^1.0.17",
        "nikic/php-parser": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Aop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Aop\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml",
        "cs-fix": "./vendor/bin/php-cs-fixer fix $1"
    }
}
